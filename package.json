{
  "name": "@nutriot/responsive-ratio",
  "description": "Library to adjust iFrame aspect-ratios reponsively",
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "npm run start",
    "fix": "eslint --fix ./src/index.ts",
    "lint": "eslint ./src/index.ts",
    "prepublishOnly": "npm run lint && npm run build",
    "start": "rollup -c -w",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nutriot/responsive-ratio"
  },
  "bugs": {
    "url": "https://github.com/nutriot/responsive-ratio"
  },
  "keywords": [
    "aspect ratio",
    "iframe",
    "ratio",
    "react",
    "responsive iframe",
    "responsive"
  ],
  "dependencies": {
    "nano-css": "^5.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.2.5",
    "rollup": "^2.23.0",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
